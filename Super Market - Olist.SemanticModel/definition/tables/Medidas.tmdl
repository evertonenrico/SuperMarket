	measure '06 - TotalPriceFreight' =
			
			SUMX(
			    Fact_OrderItemsDataset,
			    Fact_OrderItemsDataset[price] + Fact_OrderItemsDataset[freight_value]
			)
		formatString: "R$"\ #,0.###############;-"R$"\ #,0.###############;"R$"\ #,0.###############
